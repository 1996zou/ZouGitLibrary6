function GetDataTypeByParamName(AParamName: string): TCMBasicType;
function GetLimit(nFunction: Integer): Boolean;

implementation

uses uDllDataBaseIntf, uDllDBService, Db, DBClient, uDllComm, uDllSystemIntf;

function GetDataTypeByParamName(AParamName: string): TCMBasicType;
var
  I: TCMBasicType;
begin
  for I := Low(TCMBasicType) to High(TCMBasicType) do
  begin
    if AnsiCompareText(GetGlobalDataType(I).ParamName, AParamName) = 0 then
    begin
      Result := I;
      Exit;
    end;
  end;
  Result := CMbtNo;
end;

